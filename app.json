{
  "expo": {
    "name": "animalesco_frontend",
    "slug": "animalesco_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo_without_background.png",
      "resizeMode": "contain",
      "backgroundColor": "#f15a24"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_without_background.png",
        "backgroundColor": "#f15a24"
      }
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    }
  }
}
